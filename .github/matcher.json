{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/refs/heads/master/src/schemas/json/task.json",
  "problemMatcher": [
    {
      "owner": "changesets",
      "pattern": [{ "regexp": "^ğŸ¦‹\\s+(\\x1B\\[\\d+m)?(error|warn)(\\x1B\\[\\d+m)?\\s+(.*)$", "message": 4 }]
    },
    {
      "owner": "cspell",
      "severity": "warning",
      "pattern": [{ "regexp": "^(.*):(\\d+):(\\d+) - (.*)$", "file": 1, "line": 2, "column": 3, "message": 4 }]
    },
    {
      "owner": "markdownlint",
      "pattern": [
        {
          "regexp": "^([^:]*):\\s*(\\d+):?(\\d+)?\\s+(MD\\d{3}\/[a-z\\d/-]+)\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "pnpm-audit",
      "pattern": [{ "regexp": "^â”‚\\s(low|moderate|high|critical)\\s*â”‚\\s(.+?)\\s*â”‚$", "message": 2 }]
    },
    {
      "owner": "forge-fmt",
      "pattern": [{ "regexp": "^(Diff in (.+)):$", "file": 2, "message": 1 }]
    },
    {
      "owner": "forge-snapshot",
      "pattern": [{ "regexp": "^(Diff in \"(.+?)\":\\s+.*)$", "file": 2, "message": 1 }]
    },
    {
      "owner": "slither",
      "pattern": [
        {
          "regexp": "^(?!\\s)(.*\\(([^#]+\\/[^#]+)#(\\d+)(?:-[0-9]+)?\\).*)$",
          "file": 2,
          "line": 3,
          "message": 1
        }
      ]
    },
    {
      "owner": "rust",
      "pattern": [
        {
          "regexp": "^(?:\\x1b\\[[\\d;]+m)*([Ww]arn(?:ing)?|[Ee]rror)(?:\\x1b\\[[\\d;]+m)*(?:\\[(.*)\\])?(?:\\x1b\\[[\\d;]+m)*:(?:\\x1b\\[[\\d;]+m)*\\s+(?:\\x1b\\[[\\d;]+m)*([^\\x1b]*)(?:\\x1b\\[[\\d;]+m)*$",
          "severity": 1,
          "message": 3,
          "code": 2
        },
        {
          "regexp": "^(?:\\x1b\\[[\\d;]+m)*\\s*(?:\\x1b\\[[\\d;]+m)*\\s*-->\\s+(?:\\x1b\\[[\\d;]+m)*([^:]+):(\\d+):(\\d+)(?:\\x1b\\[[\\d;]+m)*$",
          "file": 1,
          "line": 2,
          "column": 3
        }
      ]
    },
    {
      "owner": "rust-formatter",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(?:\\x1b\\[[\\d;]+m)*(Diff in (.+)) at line (\\d+):(?:\\x1b\\[[\\d;]+m)*$",
          "message": 1,
          "file": 2,
          "line": 3
        }
      ]
    }
  ]
}
