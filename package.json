{
  "name": "@exactly/mobile",
  "version": "0.0.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prepare": "wagmi generate && node -e 'require(\"fs\").writeFileSync(\"src/generated/version.ts\", `export default \"${require(\"./package.json\").version}\" as const;`)'",
    "eas-build-pre-install": "export FOUNDRY_DIR=${FOUNDRY_DIR-$HOME/workingdir} && curl -L https://foundry.paradigm.xyz | bash || true && foundryup",
    "eas-build-on-success": "expo-updates fingerprint:generate --debug --platform android",
    "test": "pnpm run --aggregate-output /test:/",
    "test:deps": "pnpm audit",
    "test:expo": "expo-doctor",
    "test:build": "expo export --platform web",
    "test:ts:expo": "tsc",
    "test:ts:node": "tsc -p tsconfig.node.json",
    "test:eslint": "eslint --max-warnings 0 ."
  },
  "engines": {
    "node": ">=22.9.0"
  },
  "dependencies": {
    "@alchemy/aa-accounts": "^3.19.0",
    "@alchemy/aa-alchemy": "^3.19.0",
    "@alchemy/aa-core": "^3.19.0",
