    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/eslint-plugin": "^1.6.4",
    "babel-plugin-istanbul": "^7.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^12.0.11",
    "cspell": "^8.19.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-config-universe": "^14.0.0",
    "eslint-import-resolver-typescript": "^4.3.3",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-dev-client": "~5.0.18",
    "expo-doctor": "^1.12.8",
    "gitmojis": "^3.15.0",
    "markdownlint-cli": "^0.44.0",
    "metro-config": "^0.81.0",
    "nx": "22.1.3",
    "onesignal-expo-plugin": "^2.0.3",
    "prettier": "^3.5.3",
    "react-native-svg-transformer": "^1.5.2",
    "sharp": "^0.34.1",
    "sharp-cli": "^2.1.1",
    "svgo": "^3.3.2",
    "typescript": "^5.8.3",
    "ws": "^8.18.1"
  },
  "packageManager": "pnpm@9.15.9",
  "pnpm": {
    "overrides": {
      "@tanstack/react-form@0.19.5": "^0.42.1",
      "@valibot/to-json-schema": "^1.3.0",
      "@wagmi/core": "$@wagmi/core",
      "abitype": "$abitype",
      "axios@1.8.4": "^1.12.0",
      "brace-expansion@1.1.11": "^1.1.12",
      "brace-expansion@2.0.1": "^2.0.2",
      "cookie": "^0.7.0",
      "elliptic": "^6.5.7",
      "esbuild": "^0.25.0",
      "form-data@2.5.3": "^2.5.4",
      "form-data@3.0.3": "^3.0.4",
      "form-data@4.0.2": "^4.0.4",
      "glob@10.4.5": "^10.5.0",
      "mdast-util-to-hast@13.2.0": "^13.2.1",
      "nanoid@3.3.7": "^3.3.8",
      "node-forge@1.3.1": "^1.3.2",
      "on-headers@1.0.2": "^1.1.0",
      "react-native": "$react-native",
      "semver@7.3.2": "^7.5.2",
      "send@0.18.0": "^0.19.0",
      "sharp": "$sharp",
      "tmp@0.0.33": "^0.2.4",
      "typescript": "$typescript",
      "unicorn-magic@0.3.0": "^0.2.0",
      "valibot": "$valibot",
      "viem": "$viem",
      "wagmi": "$wagmi",
      "ws": "$ws",
      "yargs@12.0.5": "^13.3.0",
      "zustand": "$zustand",
      "@bigmi/core": "npm:@favware/skip-dependency@1.2.2",
      "@gnosis.pm/safe-contracts": "npm:@favware/skip-dependency@1.2.2",
      "@mysten/sui": "npm:@favware/skip-dependency@1.2.2",
      "@mysten/wallet-standard": "npm:@favware/skip-dependency@1.2.2",
      "@nomiclabs/hardhat-etherscan": "npm:@favware/skip-dependency@1.2.2",
      "@solana/wallet-adapter-base": "npm:@favware/skip-dependency@1.2.2",
      "@solana/web3.js": "npm:@favware/skip-dependency@1.2.2",
      "@tanstack/zod-form-adapter": "npm:@favware/skip-dependency@1.2.2",
      "@thehubbleproject/bls": "npm:@favware/skip-dependency@1.2.2",
      "@turnkey/http": "npm:@favware/skip-dependency@1.2.2",
      "@turnkey/iframe-stamper": "npm:@favware/skip-dependency@1.2.2",
      "@turnkey/viem": "npm:@favware/skip-dependency@1.2.2",
      "@turnkey/webauthn-stamper": "npm:@favware/skip-dependency@1.2.2",
      "@typechain/hardhat": "npm:@favware/skip-dependency@1.2.2",
      "@types/mocha": "npm:@favware/skip-dependency@1.2.2",
      "@wagmi/connectors": "npm:@favware/skip-dependency@1.2.2",
      "bech32": "npm:@favware/skip-dependency@1.2.2",
      "bs58": "npm:@favware/skip-dependency@1.2.2",
      "bigint-buffer": "npm:@favware/skip-dependency@1.2.2",
      "bitcoinjs-lib": "npm:@favware/skip-dependency@1.2.2",
      "ethereumjs-util": "npm:@favware/skip-dependency@1.2.2",
      "ethereumjs-wallet": "npm:@favware/skip-dependency@1.2.2",
      "hardhat-deploy": "npm:@favware/skip-dependency@1.2.2",
      "hardhat-deploy-ethers": "npm:@favware/skip-dependency@1.2.2",
      "solidity-coverage": "npm:@favware/skip-dependency@1.2.2"
    },
    "patchedDependencies": {
      "eslint-config-universe": "patches/eslint-config-universe.patch",
      "@lifi/sdk": "patches/@lifi__sdk.patch",
      "react-native-svg-transformer": "patches/react-native-svg-transformer.patch"
    },
    "updateConfig": {
      "ignoreDependencies": [
        "@openzeppelin/contracts-v4",
        "@openzeppelin/contracts-upgradeable-v4",
        "@react-native-async-storage/async-storage",
        "@react-navigation/bottom-tabs",
        "@tamagui/animations-moti",
        "@tamagui/babel-plugin",
        "@tamagui/config",
        "@tamagui/lucide-icons",
        "@tamagui/toast",
        "@tanstack/react-form",
        "@types/react",
        "account-abstraction",
        "eslint",
        "eslint-plugin-unicorn",
        "expo-asset",
        "expo-build-properties",
        "expo-clipboard",
        "expo-dev-client",
        "expo-font",
        "expo-insights",
        "expo-linear-gradient",
        "expo-linking",
        "expo-router",
        "expo-status-bar",
        "expo-system-ui",
        "expo-updates",
        "expo-web-browser",
        "metro-config",
        "react",
        "react-dom",
        "react-native",
        "react-native-persona",
        "react-native-reanimated",
        "react-native-safe-area-context",
        "react-native-screens",
        "sharp-cli",
        "tamagui"
      ]
    },
    "neverBuiltDependencies": [],
    "peerDependencyRules": {
      "ignoreMissing": [
        "hardhat",
        "typechain"
      ]
    },
    "auditConfig": {
      "ignoreGhsas": [
        "GHSA-mh29-5h37-fv8m"
      ]
    }
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@intercom/intercom-react-native",
          "tamagui"
        ],
        "listUnknownPackages": false
      }
    },
    "install": {
      "exclude": [
        "@sentry/react-native",
        "react-native-svg",
        "react-native-webview",
        "typescript"
      ]
    }
  },
  "nx": {
    "projectType": "application",
    "namedInputs": {
      "default": [
        "{projectRoot}/patches/**/*",
        "{projectRoot}/src/**/*",
        "sharedGlobals",
        "!{projectRoot}/common/**/*",
        "!{projectRoot}/contracts/**/*",
        "!{projectRoot}/docs/**/*",
        "!{projectRoot}/server/**/*",
        "!{projectRoot}/src/generated/**/*",
        {
          "env": "APP_DOMAIN"
        },
        {
          "env": "EAS_BUILD_RUNNER"
        },
        {
          "env": "EXPO_PUBLIC_INTERCOM_APP_ID"
        },
        {
          "env": "NODE_ENV"
        }
      ]
    },
    "targets": {
      "generate:expo": {
        "dependsOn": [
          "generate:version"
        ],
        "outputs": [
          "{projectRoot}/expo-env.d.ts"
        ]
      },
      "generate:release": {
        "dependsOn": [
          "prepare:generated",
          {
            "projects": "@exactly/common",
            "target": "git:unshallow"
          }
        ],
        "inputs": [],
        "outputs": [
          "{projectRoot}/generated/release.js"
        ],
        "cache": false
      },
      "generate:version": {
        "dependsOn": [
          "prepare:generated",
          {
            "projects": "@exactly/common",
            "target": "git:unshallow"
          }
        ],
        "inputs": [],
        "outputs": [
          "{projectRoot}/generated/versionCode.js"
        ],
        "cache": false
      },
      "start": {
        "dependsOn": [
          "prepare",
          "^prepare"
        ],
        "inputs": [
          "default",
          "^production"
        ],
        "continuous": true
      },
      "android": {
        "dependsOn": [
          "prepare",
          "^prepare"
        ],
        "inputs": [
          "default",
          "^production"
        ],
        "continuous": true
      },
      "ios": {
        "dependsOn": [
          "prepare",
          "^prepare"
        ],
        "inputs": [
          "production",
          "^production"
        ],
        "continuous": true
      },
      "web": {
        "dependsOn": [
          "prepare",
          "^prepare"
        ],
        "inputs": [
          "production",
          "^production"
        ],
        "continuous": true
      },
      "e2e:ios": {
        "inputs": [
          "production",
          "^production"
        ],
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "rm -rf build/Exa.app build/Exa.tar.gz",
            "pnpx eas-cli build --platform=ios --profile=e2e --local --output=build/Exa.tar.gz",
            "open -a Simulator",
            "tar -xzf build/Exa.tar.gz -C build",
            "xcrun simctl install booted build/Exa.app"
          ],
          "parallel": false
        }
      },
      "test": {
        "dependsOn": [
          "test:spell",
          "test:deps",
          "test:build",
          "test:ts:expo",
          "test:ts:node",
          "test:markdown",
          "test:changeset",
          "test:eslint"
        ],
        "executor": "nx:noop"
      }
    }
  },
  "private": true
}
