        "eslint",
        "expo-build-properties",
        "expo-clipboard",
        "expo-constants",
        "expo-font",
        "expo-linking",
        "expo-router",
        "expo-status-bar",
        "react",
        "react-dom",
        "react-native",
        "react-native-reanimated",
        "react-native-safe-area-context",
        "react-native-screens",
        "react-native-svg",
        "react-native-webview",
        "sharp-cli"
      ]
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "hardhat",
        "typechain"
      ]
    }
  },
  "expo": {
    "install": {
      "exclude": [
        "@sentry/react-native",
        "typescript"
      ]
    }
  },
  "private": true
}
