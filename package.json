    "fast-text-encoding": "^1.0.6",
    "hono": "^4.6.3",
    "i18n-iso-countries": "^7.12.0",
    "moti": "^0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-onesignal": "^5.2.4",
    "react-native-passkeys": "github:cruzdanilo/react-native-passkeys#a6bf796",
