    "fast-text-encoding": "^1.0.6",
    "moti": "^0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-onesignal": "^5.2.2",
    "react-native-passkeys": "github:cruzdanilo/react-native-passkeys#c745e8f",
    "react-native-persona": "^2.9.2",
    "react-native-quick-crypto": "^0.7.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "~15.2.0",
    "react-native-webview": "13.8.6",
    "react-onesignal": "^3.0.1",
    "tamagui": "^1.108.3",
    "valibot": "^0.37.0",
    "viem": "^2.19.4",
