{
  "name": "@exactly/mobile",
  "version": "1.0.43",
  "title": "Exa App",
  "shortDescription": "What finance should be today",
  "description": "A Card. A Wallet. A DeFi Protocol. All of it together.",
  "main": "./src/index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo export --platform web --no-minify --source-maps",
    "prepare": "nx run-many -t generate:expo generate:release generate:version -p mobile",
    "prepare:generated": "mkdir -p src/generated",
    "generate:expo": "expo customize tsconfig.json",
    "generate:release": "echo \"module.exports = /** @type {const} */ ('$(git describe --tags --abbrev=7 --match '@exactly/mobile@*' | sed 's/@exactly\\///')')\" > src/generated/release.js",
    "generate:version": "echo \"module.exports = /** @type {const} */ ($(git rev-list --count HEAD))\" > src/generated/versionCode.js",
    "eas-build-on-success": "[ \"$EAS_BUILD_PROFILE\" != production ] || sentry-cli releases new --finalize -o exactly -p exa $(node -p 'require(\"./src/generated/release\")')",
    "e2e:iphone": "maestro start-device --platform=ios --os-version=26 --device-locale=es-419",
    "e2e:maestro": "maestro test .maestro/flows --include-tags=critical",
    "test": "nx test mobile",
    "test:spell": "cspell --gitignore '**' '.changeset/**' '.cursor/**' '.do/**' '.github/**' '.eslintrc.*'",
    "test:deps": "pnpm audit",
    "test:build": "expo export --platform web",
    "test:ts:expo": "tsc --pretty ${GITHUB_ACTIONS:+false}",
    "test:ts:node": "tsc -p tsconfig.node.json --pretty ${GITHUB_ACTIONS:+false}",
    "test:markdown": "markdownlint '**/*.md' '**/*.mdx' '.changeset/**/*.md' '.cursor/**/*.mdc' --ignore ios",
    "test:changeset": "changeset status --since origin/main",
    "test:eslint": "eslint --max-warnings 0 ."
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "dependencies": {
    "@alchemy/aa-accounts": "^3.19.0",
    "@alchemy/aa-alchemy": "^3.19.0",
    "@alchemy/aa-core": "^3.19.0",
    "@exactly/common": "workspace:^0.0.10",
    "@exactly/lib": "^0.1.0",
    "@exactly/server": "workspace:^0.2.45",
    "@expo/metro-runtime": "~4.0.1",
    "@farcaster/frame-sdk": "^0.1.12",
    "@farcaster/miniapp-sdk": "^0.2.1",
    "@farcaster/miniapp-wagmi-connector": "^1.1.0",
