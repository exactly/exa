    "@tanstack/query-persist-client-core": "^5.51.21",
    "@tanstack/react-form": "^0.29.1",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-query-persist-client": "^5.51.23",
    "@tanstack/valibot-form-adapter": "^0.28.0",
    "@wagmi/core": "^2.13.4",
