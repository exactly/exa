    "@tanstack/react-query-persist-client": "^5.74.4",
    "@wagmi/core": "^2.22.1",
    "abitype": "^1.2.0",
    "buffer": "^6.0.3",
    "burnt": "^0.13.0",
    "date-fns": "^4.1.0",
    "expo": "~52.0.46",
    "expo-asset": "~11.0.4",
    "expo-camera": "^16.0.18",
    "expo-clipboard": "~7.0.0",
    "expo-font": "~13.0.4",
    "expo-insights": "~0.8.1",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.1",
    "fast-text-encoding": "^1.0.6",
    "hono": "^4.10.3",
    "i18n-iso-countries": "^7.14.0",
    "i18next": "^25.1.3",
    "moti": "^0.30.0",
