  "pnpm": {
    "overrides": {
      "@expo/image-utils": "$@expo/image-utils",
      "@tanstack/react-form": "$@tanstack/react-form",
      "@wagmi/core": "$@wagmi/core",
      "abitype": "$abitype",
      "elliptic": "^6.5.7",
      "react-native": "$react-native",
      "send@0.18.0": "^0.19.0",
      "sharp": "$sharp",
      "typescript": "$typescript",
      "valibot": "$valibot",
      "viem": "$viem",
      "wagmi": "$wagmi",
      "ws": "$ws",
      "yargs@12.0.5": "^13.3.0",
      "zustand": "$zustand",
      "@gnosis.pm/safe-contracts": "npm:@favware/skip-dependency@1.2.2",
