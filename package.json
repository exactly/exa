    "@react-navigation/bottom-tabs": "^7.0.0",
    "@sentry/browser": "^9.13.0",
    "@sentry/react-native": "^6.10.0",
    "@simplewebauthn/browser": "^13.1.0",
    "@tamagui/animations-moti": "1.125.26",
    "@tamagui/config": "1.125.26",
    "@tamagui/lucide-icons": "1.125.26",
