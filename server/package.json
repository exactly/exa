{
  "name": "@exactly/server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "pkgroll --sourcemap --src .",
    "sourcemaps": "sentry-cli sourcemaps inject -o exactly -p mobile dist && sentry-cli sourcemaps upload -o exactly -p mobile dist",
    "db:push": "drizzle-kit push",
    "test": "pnpm run --aggregate-output /test:/",
    "test:ts": "tsc",
    "test:vi": "vitest run",
    "dev": "tsx --watch --env-file=.env --require=./instrument.cjs index.ts"
  },
  "bin": "./dist/index.cjs",
  "files": [
    ".well-known",
    "app",
    "dist",
    "instrument.cjs"
  ],
  "engines": {
    "node": ">=22.13.1"
  },
  "dependencies": {
