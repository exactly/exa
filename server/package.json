    "@hono/valibot-validator": "^0.5.2",
    "@onesignal/node-onesignal": "^5.0.0-alpha-02",
    "@segment/analytics-node": "^2.2.1",
    "@sentry/node": "^9.2.0",
    "@sentry/profiling-node": "^9.2.0",
    "@simplewebauthn/server": "^13.1.1",
    "@types/debug": "^4.1.12",
    "async-mutex": "^0.5.0",
    "debug": "^4.4.0",
    "drizzle-orm": "^0.40.0",
    "graphql": "^16.10.0",
    "hono": "^4.7.2",
