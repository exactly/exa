{
  "installCommand": "curl -L https://foundry.paradigm.xyz | bash && ${SHELL-bash} -c 'foundryup && bun install'",
  "buildCommand": "if [ -z \"${__VERCEL_DEV_RUNNING+1}\" ]; then bun run build:app; fi",
  "outputDirectory": "/",
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [{ "source": "/:path((?!api/).*)", "destination": "/app/:path*" }],
  "headers": [{ "source": "/.well-known/(.*)", "headers": [{ "key": "Content-Type", "value": "application/json" }] }],
  "$schema": "https://openapi.vercel.sh/vercel.json"
}
