{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ESNext",
    "lib": ["ESNext"],
    "pretty": true,
    "strict": true,
    "checkJs": true,
    "noUncheckedIndexedAccess": true
  },
  "include": [
    ".expo/types/**/*.ts",
    "common/**/*.ts",
    "expo-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "tamagui.config.ts"
  ]
}
